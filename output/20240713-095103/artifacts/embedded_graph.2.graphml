      <data key="d6">4a396711762b9d23dfa301a027d2737e</data>
    </edge>
    <edge source="&quot;COLOR REVOLUTIONS&quot;" target="&quot;EUROMAIDAN REVOLT&quot;">
      <data key="d4">1.0</data>
      <data key="d5">"The Euromaidan Revolt is another example of a Color Revolution that involved violence."</data>
      <data key="d6">127b37863425efc735533b75512a27a3</data>
    </edge>
